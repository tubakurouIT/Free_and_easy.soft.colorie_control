

<% unless current_member.email == "guest@example.com" %>
<h2 class="mt-3">New free_post</h2>
<%= form_with model:free_post,local:true do |f| %>
  

  <div class="form-group">
    <%= f.label :image %>
    <%= f.file_field :image, class: "form-control-file free_post_image", accept: 'image/*' %></td>
  </div>

  <div class="form-group">
    <%= f.label :body %>
    <%= f.text_area :body, class: 'form-control free_post_body' %>
  </div>
  <div class="form-group">
    <%= f.submit class: 'btn btn-success' %>
  <% if free_post.id %>
    <%= link_to "削除", free_post_path(free_post), method: :delete,class:"btn btn-danger" %>
  <% end %>
  </div>

    
<% end %>
<% end %>