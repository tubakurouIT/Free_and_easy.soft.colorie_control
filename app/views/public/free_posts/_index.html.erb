<table class='table table-hover table-inverse'>
  <thead>
    <tr>
      <th></th>
      <th>free_posts</th>
      
      <th colspan="3"></th>
    </tr>
  </thead>
  <tbody>
    <% free_posts.each do |free_post| %>
      <tr>
        <td><%= link_to(free_post.member) do %>
          <%= image_tag free_post.member.get_profile_image, size:'50x50' %>
          <% end %>
        </td>
        
        <td><%= free_post.body %></td>
        
       
      </tr>
    <% end %>
  </tbody>
</table>